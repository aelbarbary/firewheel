{
  "name": "react-redux-template-for-ecommerce-site",
  "version": "0.1.0",
  "description": "A beautiful e-commerce template with a flat design.",
  "engines": {
    "npm": ">=3"
  },
  "scripts": {
    "preinstall": "node tools/nodeVersionCheck.js",
    "setup": "node tools/setup/setupMessage.js && npm install && node tools/setup/setup.js",
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message",
    "start": "npm-run-all --parallel open:src lint:watch",
    "open:src": "babel-node tools/server.js development",
    "open:dist": "babel-node tools/server.js production",
    "lint": "esw webpack.config.* src tools --color",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm run clean-dist && npm run lint && npm run test",
    "build": "babel-node tools/build.js && npm run open:dist",
    "test": "node --harmony node_modules/jest/bin/jest",
    "test:cover": "npm run test -- --coverage ",
    "test:cover:travis": "npm run test:cover && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "test:watch": "npm run test -- --watch",
    "open:cover": "npm run test:cover && open coverage/lcov-report/index.html",
    "analyze-bundle": "babel-node ./tools/analyzeBundle.js"
  },
  "author": "Arshad Khan <arshdkhn1@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "babel-eslint": "8.2.3",
    "body-parser": "^1.18.2",
    "dotenv": "^5.0.1",
    "express-session": "^1.15.6",
    "mongoose": "^5.0.17",
    "object-assign": "4.1.1",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth": "^1.0.0",
    "passport-twitter": "^1.0.4",
    "postcss-loader": "2.1.4",
    "prop-types": "15.6.1",
    "react": "16.3.2",
    "react-addons-css-transition-group": "15.6.2",
    "react-dom": "16.3.2",
    "react-redux": "5.0.7",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2",
    "react-router-redux": "4.0.8",
    "react-tap-event-plugin": "^3.0.2",
    "redux": "4.0.0",
    "redux-thunk": "2.2.0"
  },
  "devDependencies": {
    "autoprefixer": "8.4.1",
    "axios": "0.18.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-jest": "22.4.3",
    "babel-loader": "7.1.4",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.13",
    "babel-polyfill": "6.26.0",
    "babel-preset-latest": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-1": "6.24.1",
    "browser-sync": "2.24.2",
    "chalk": "2.4.1",
    "connect-history-api-fallback": "1.5.0",
    "coveralls": "3.0.1",
    "css-loader": "0.28.11",
    "enzyme": "3.3.0",
    "eslint": "4.19.1",
    "eslint-plugin-import": "2.11.0",
    "eslint-plugin-react": "7.7.0",
    "eslint-watch": "3.1.4",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "1.1.11",
    "html-webpack-plugin": "3.2.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "22.4.3",
    "json-loader": "0.5.7",
    "mockdate": "2.0.2",
    "node-sass": "4.9.0",
    "npm-run-all": "4.1.2",
    "open": "0.0.5",
    "prompt": "1.0.0",
    "react-addons-test-utils": "15.6.2",
    "redux-immutable-state-invariant": "2.1.0",
    "redux-logger": "3.0.6",
    "redux-persist": "5.9.1",
    "replace": "0.3.0",
    "rimraf": "2.6.2",
    "sass-loader": "7.0.1",
    "style-loader": "0.21.0",
    "url-loader": "1.0.1",
    "webpack": "4.6.0",
    "webpack-bundle-analyzer": "2.11.1",
    "webpack-dev-middleware": "3.1.3",
    "webpack-hot-middleware": "2.22.1",
    "webpack-md5-hash": "0.0.6"
  },
  "keywords": [
    "react",
    "redux",
    "ecommerce",
    "Arshad Khan",
    "react-slingshot",
    "express",
    "node"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/arshdkhn1/react-redux-template-for-ecommerce-site.git"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|sass)$": "<rootDir>/__mocks__/styleMock.js",
      "^.+\\.(gif|ttf|eot|svg|woff|woff2|ico)$": "<rootDir>/tools/fileMock.js"
    }
  }
}
